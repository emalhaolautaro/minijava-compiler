.DATA
VT@TestIfReturn: NOP

.CODE
lbl_builder@TestIfReturn: LOADFP
LOADSP
STOREFP
STOREFP
RET 1
lbl_max@TestIfReturn: LOADFP
LOADSP
STOREFP
LOAD 3 ; Cargar parámetro a
LOAD 4 ; Cargar parámetro b
GT
BF lbl_finif1
LOAD 3 ; Cargar parámetro a
STORE 5 ; Guardar valor de retorno en M[fp+5]
JUMP lbl_end_max@TestIfReturn ; salto al final del método tras return
lbl_finif1: NOP
LOAD 4 ; Cargar parámetro b
STORE 5 ; Guardar valor de retorno en M[fp+5]
JUMP lbl_end_max@TestIfReturn ; salto al final del método tras return
lbl_end_max@TestIfReturn: NOP
STOREFP
RET 2
lbl_main@TestIfReturn: LOADFP
LOADSP
STOREFP
RMEM 1 ; Reservar espacio para valor de retorno
PUSH 10
PUSH 3
PUSH lbl_max@TestIfReturn
CALL
PUSH lbl_debugPrint@Object
CALL
RMEM 1 ; Reservar espacio para valor de retorno
PUSH 2
PUSH 7
PUSH lbl_max@TestIfReturn
CALL
PUSH lbl_debugPrint@Object
CALL
lbl_end_main@TestIfReturn: NOP
STOREFP
RET 0

