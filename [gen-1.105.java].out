.DATA
VT@Adder: DW lbl_sumar@Adder

.CODE
lbl_builder@Adder: LOADFP
LOADSP
STOREFP
LOAD 4 ; Cargar parámetro a
LOAD 5 ; Cargar parámetro b
ADD
LOAD 3
SWAP
STOREREF 1
STOREFP
RET 3
lbl_sumar@Adder: LOADFP
LOADSP
STOREFP
LOAD 3 ; Cargar this
LOADREF 1 ; Cargar atributo total
STORE 3 ; Guardar valor de retorno en M[fp+3]
JUMP lbl_end_sumar@Adder ; salto al final del método tras return
lbl_end_sumar@Adder: NOP
STOREFP
RET 0

.DATA
VT@TestAdder: NOP

.CODE
lbl_builder@TestAdder: LOADFP
LOADSP
STOREFP
STOREFP
RET 1
lbl_main@TestAdder: LOADFP
LOADSP
STOREFP
RMEM 2 ; Reservar 2 celdas locales
PUSH 1
PUSH lbl_printIln@System
CALL
RMEM 1 ; Reservar espacio para el puntero 'new'
PUSH 2 ; Apilar tamaño del CIR (k)
PUSH simple_malloc
CALL
DUP
PUSH VT@Adder
STOREREF 0 ; Guardar puntero a la VT en offset 0
DUP
PUSH 10
SWAP ; Mover 'this' debajo del argumento
PUSH 20
SWAP ; Mover 'this' debajo del argumento
PUSH lbl_builder@Adder
CALL
STORE 0 ; Guardar variable local a1
FMEM 1 ; Liberar slot temporal del 'new'
LOAD 0 ; Cargar variable local a1
DUP ; duplicar ref obj
LOADREF 0 ; cargar VT
LOADREF 0 ; cargar dir metodo sumar
CALL
PUSH lbl_printIln@System
CALL
RMEM 1 ; Reservar espacio para el puntero 'new'
PUSH 2 ; Apilar tamaño del CIR (k)
PUSH simple_malloc
CALL
DUP
PUSH VT@Adder
STOREREF 0 ; Guardar puntero a la VT en offset 0
DUP
PUSH 50
SWAP ; Mover 'this' debajo del argumento
PUSH 50
SWAP ; Mover 'this' debajo del argumento
PUSH lbl_builder@Adder
CALL
STORE -1 ; Guardar variable local a2
FMEM 1 ; Liberar slot temporal del 'new'
LOAD -1 ; Cargar variable local a2
DUP ; duplicar ref obj
LOADREF 0 ; cargar VT
LOADREF 0 ; cargar dir metodo sumar
CALL
PUSH lbl_printIln@System
CALL
PUSH 2
PUSH lbl_printIln@System
CALL
lbl_end_main@TestAdder: NOP
FMEM 2 ; Liberar celdas locales
STOREFP
RET 0

