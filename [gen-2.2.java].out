.CODE
PUSH simple_heap_init
CALL
PUSH lbl_main@Init
CALL
HALT

simple_heap_init: RET 0

simple_malloc: LOADFP
LOADSP
STOREFP
LOADHL
DUP
PUSH 1
ADD
STORE 4
LOAD 3
ADD
STOREHL
STOREFP
RET 1

.DATA
VT@Init: NOP

.CODE
lbl_builder@Init: LOADFP
LOADSP
STOREFP
STOREFP
RET 1
lbl_main@Init: LOADFP
LOADSP
STOREFP
RMEM 1 ; Reservar 1 celdas locales
RMEM 1 ; Reservar espacio para el puntero 'new'
PUSH 3 ; Apilar tamaño del CIR (k)
PUSH simple_malloc
CALL
DUP
PUSH VT@A
STOREREF 0 ; Guardar puntero a la VT en offset 0
DUP
PUSH lbl_builder@A
CALL
STORE 0 ; Guardar variable local a
FMEM 1 ; Liberar slot temporal del 'new'
LOAD 0 ; Cargar variable local a
DUP ; duplicar ref obj
LOADREF 0 ; cargar VT
LOADREF 0 ; cargar dir metodo mc
CALL
LOAD 0 ; Cargar variable local a
LOADREF 1 ; cargar atributo x
PUSH lbl_printIln@System
CALL
LOAD 0 ; Cargar variable local a
LOADREF 2 ; cargar atributo y
PUSH lbl_printIln@System
CALL
lbl_end_main@Init: NOP
FMEM 1 ; Liberar celdas locales
STOREFP
RET 0

