.DATA
VT@A: NOP

.CODE
lbl_builder@A: LOADFP
LOADSP
STOREFP
STOREFP
RET 1
lbl_main@A: LOADFP
LOADSP
STOREFP
RMEM 2 ; Reservar 2 celdas locales
PUSH 0
STORE 0 ; Guardar variable local x
lbl_loop_ini1: NOP
LOAD 0 ; Cargar variable local x
PUSH 1
LT
BF lbl_loop_fin1
PUSH 5
STORE -1 ; Guardar variable local y
LOAD -1 ; Cargar variable local y
PUSH lbl_printIln@System
CALL
LOAD 0 ; Cargar variable local x
PUSH 1
ADD
STORE 0
JUMP lbl_loop_ini1
lbl_loop_fin1: NOP
LOAD 0 ; Cargar variable local x
PUSH lbl_printIln@System
CALL
lbl_end_main@A: NOP
FMEM 2 ; Liberar celdas locales
STOREFP
RET 0

