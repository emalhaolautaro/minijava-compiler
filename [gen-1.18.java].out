.CODE
PUSH simple_heap_init
CALL
PUSH lbl_main@A
CALL
HALT

simple_heap_init: RET 0

simple_malloc: LOADFP
LOADSP
STOREFP
LOADHL
DUP
PUSH 1
ADD
STORE 4
LOAD 3
ADD
STOREHL
STOREFP
RET 1

