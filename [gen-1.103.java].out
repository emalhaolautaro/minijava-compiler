.DATA
VT@TestWhileReturn: NOP

.CODE
lbl_builder@TestWhileReturn: LOADFP
LOADSP
STOREFP
STOREFP
RET 1
lbl_sumaHasta@TestWhileReturn: LOADFP
LOADSP
STOREFP
RMEM 1 ; Reservar 1 celdas locales
PUSH 0
STORE 0 ; Guardar variable local acc
lbl_loop_ini1: NOP
LOAD 3 ; Cargar parámetro n
PUSH 0
GT
BF lbl_loop_fin1
LOAD 0 ; Cargar variable local acc
LOAD 3 ; Cargar parámetro n
ADD
STORE 0
LOAD 3 ; Cargar parámetro n
PUSH 1
SUB
STORE 3
JUMP lbl_loop_ini1
lbl_loop_fin1: NOP
LOAD 0 ; Cargar variable local acc
STORE 4 ; Guardar valor de retorno en M[fp+4]
JUMP lbl_end_sumaHasta@TestWhileReturn ; salto al final del método tras return
lbl_end_sumaHasta@TestWhileReturn: NOP
FMEM 1 ; Liberar celdas locales
STOREFP
RET 1
lbl_main@TestWhileReturn: LOADFP
LOADSP
STOREFP
RMEM 1 ; Reservar espacio para valor de retorno
PUSH 5
PUSH lbl_sumaHasta@TestWhileReturn
CALL
PUSH lbl_debugPrint@Object
CALL
lbl_end_main@TestWhileReturn: NOP
STOREFP
RET 0

